{
    "extends": "standard",
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [ "html", "standard" ],
    "rules": {
        "indent": 0,//["error", 4, { "SwitchCase": 1 }]
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": 0, // 0  ignore 1 warning 2 error
        "no-empty": 2,
        "no-eq-null": 2,
        "no-new": 0,
        "no-fallthrough": 0,
        "no-unreachable": 0,
        "space-before-function-paren": 0,
        "newline-after-var":0,
        "no-trailing-spaces":0,
        "space-infix-ops":0,
        "comma-dangle":0,
        "space-before-blocks":0,
        "keyword-spacing":0,
        "quotes":0,
        "space-in-parens":0,
        "comma-spacing":0,
        "key-spacing":0,
        "no-useless-escape":0,
        "eol-last":0,
        "semi": [0, "never"],
         "semi-spacing": 0,
        "no-unused-vars":0,
        "import/import-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 0,
      "import/prefer-default-export": 0,
      "import/extensions": 0
    }
}
